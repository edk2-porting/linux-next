};

&i2c8 {
	clock-frequency = <400000>;

	status = "okay";

	touchscreen@5d {
		compatible = "hid-over-i2c";
		reg = <0x5d>;

		hid-descr-addr = <0x1>;
		interrupts-extended = <&tlmm 34 IRQ_TYPE_LEVEL_LOW>;

		pinctrl-0 = <&tsp_default>;
		pinctrl-names = "default";
	};
};

&i2c9 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x33 EMEC Samsung EmuEC Device */
};

&i2c13 {
	clock-frequency = <400000>;

	status = "okay";

	touchpad@50 {
		compatible = "hid-over-i2c";
		reg = <0x50>;

		hid-descr-addr = <0xd1>;
		interrupts-extended = <&tlmm 3 IRQ_TYPE_LEVEL_LOW>;

		pinctrl-0 = <&tpad_default>;
		pinctrl-names = "default";
	};
};

&i2c15 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x33 EMEC Samsung EmuEC Device */
};

&i2c18 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x4f NXP PTN3222 (EMEC Samsung EmuEC Device) */
};

&i2c20 {
	clock-frequency = <100000>;

	status = "okay";

	/* 0x9, 0xb EMEC Samsung EmuEC Device */
};

