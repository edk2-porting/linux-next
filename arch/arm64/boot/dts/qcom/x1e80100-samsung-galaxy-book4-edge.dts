&dispcc {
	status = "disabled";
};

&i2c0 {
	clock-frequency = <400000>;

	status = "okay";

	keyboard@5 {
		compatible = "hid-over-i2c";
		reg = <0x5>;

		hid-descr-addr = <0x20>;
		interrupts-extended = <&tlmm 67 IRQ_TYPE_LEVEL_LOW>;

		pinctrl-0 = <&kybd_default>;
		pinctrl-names = "default";

		wakeup-source;
	};

	/* 0x64 ECTC Samsung EC2 Device */
};

&i2c2 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x4f NXP PTN3222 (EMEC Samsung EmuEC Device) */
};

&i2c3 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x8 UCS0 Qualcomm USB Type-C Device */
};

&i2c4 {
	clock-frequency = <400000>;

	status = "okay";

	/* 0x4f NXP PTN3222 (EMEC Samsung EmuEC Device) */
};

&i2c5 {
	clock-frequency = <400000>;

